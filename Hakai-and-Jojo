--ScriptsPRIV
local ScriptsPRIVTab = Window:MakeTab({
    Name = "ScriptsPRIV",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local Section = ScriptsPRIVTab:AddSection({
    Name = "Farm-Vida"
})
ScriptsPRIVTab:AddToggle({
    Name = "Auto-Health",
    Default = false,
    Callback = function(Value)
        _G.AutoHealth = Value
        AutoHealth()
    end
})
ScriptsPRIVTab:AddToggle({
    Name = "Auto-Health2",
    Default = false,
    Callback = function(Value)
        _G.AutoVida = Value
        AutoVida()
    end
})

ScriptsPRIVTab:AddToggle({
    Name = "Auto-Health3",
    Default = false,
    Callback = function(Value)
        _G.AutoVD = Value
        AutoVD()
    end
})
